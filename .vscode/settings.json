// Place your settings in this file to overwrite default and user settings.
{
  "git.branchProtection": [
    "main"
  ],
  "git.branchProtectionPrompt": "alwaysCommitToNewBranch",
  "editor.wordWrap": "on",
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        "**/toc.json": "jsonc"
    },
    "markdown.validate.enabled": true,
    "markdown.validate.ignoredLinks": [
        "/Download",
        "/insiders"
    ],
    "markdown.copyFiles.destination": {
        // /release-notes/v123.md -> /release-notes/images/123/img.png
        "/release-notes/**/*": "/release-notes/images/${documentBaseName/v(.*)/$1/}/",

        // Put into 'images' directory next to file
        "/api/**/*": "images/${documentBaseName}/"
    },
    "editor.codeActionsOnSave": {
        "source.organizeLinkDefinitions": true
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.cursorStyle": "underline",
    "editor.renderWhitespace": "none",
    "files.autoSave": "onWindowChange",
    "less.lint.duplicateProperties": "error",
    "less.hover.references": false,
    "less.hover.documentation": false,
    "less.format.preserveNewLines": false,
    "less.format.newlineBetweenRules": false,
    "less.format.newlineBetweenSelectors": false,
    "css.trace.server": "off",
    "css.lint.unknownProperties": "ignore",
    "css.lint.fontFaceProperties": "ignore",
    "css.lint.emptyRules": "ignore",
    "css.hover.references": false,
    "css.hover.documentation": false,
    "css.format.preserveNewLines": false,
    "css.format.newlineBetweenRules": false,
    "css.format.newlineBetweenSelectors": false,
    "css.format.enable": false,
    "css.format.braceStyle": "expand",
    "css.completion.triggerPropertyValueCompletion": false,
    "css.completion.completePropertyWithSemicolon": false,
    "azureRepos.logLevel": "off",
    "azureRepos.autoFetch.enabled": false,
    "ipynb.pasteImagesAsAttachments.enabled": false,
    "anycode.language.features": {
        "completions": false,
        "definitions": false,
        "references": false,
        "highlights": false,
        "outline": false,
        "workspaceSymbols": false
    }
}